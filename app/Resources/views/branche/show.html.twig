{% extends 'base.html.twig' %}

{% block body %}
    <h1>Branche</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ branche.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ branche.libelle }}</td>
            </tr>
            <tr>
                <th>Couleur</th>
                <td>{{ branche.couleur }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ branche.slug }}</td>
            </tr>
            <tr>
                <th>Publiepar</th>
                <td>{{ branche.publiePar }}</td>
            </tr>
            <tr>
                <th>Modifiepar</th>
                <td>{{ branche.modifiePar }}</td>
            </tr>
            <tr>
                <th>Publiele</th>
                <td>{{ branche.publieLe }}</td>
            </tr>
            <tr>
                <th>Modifiele</th>
                <td>{{ branche.modifieLe }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_branche_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_branche_edit', { 'id': branche.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
