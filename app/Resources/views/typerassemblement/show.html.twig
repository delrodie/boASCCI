{% extends 'base.html.twig' %}

{% block body %}
    <h1>Typerassemblement</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ typerassemblement.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ typerassemblement.libelle }}</td>
            </tr>
            <tr>
                <th>Couleur</th>
                <td>{{ typerassemblement.couleur }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{% if typerassemblement.statut %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ typerassemblement.slug }}</td>
            </tr>
            <tr>
                <th>Publiepar</th>
                <td>{{ typerassemblement.publiePar }}</td>
            </tr>
            <tr>
                <th>Modifiepar</th>
                <td>{{ typerassemblement.modifiePar }}</td>
            </tr>
            <tr>
                <th>Publiele</th>
                <td>{{ typerassemblement.publieLe }}</td>
            </tr>
            <tr>
                <th>Modifiele</th>
                <td>{{ typerassemblement.modifieLe }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_typerassemblement_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_typerassemblement_edit', { 'id': typerassemblement.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
